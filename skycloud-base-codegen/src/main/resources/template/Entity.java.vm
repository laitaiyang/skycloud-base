package ${package}.${moduleName}.entity;

import com.sky.framework.web.support.BaseEntity;

import lombok.Data;
import lombok.EqualsAndHashCode;
#if(${hasBigDecimal})
import java.math.BigDecimal;
#end
#if(${hasDate})
import java.sql.Date;
#end
#if(${hasTimestamp})
import java.sql.Timestamp;
#end
#if(${hasUtilDate})
import java.util.Date;
import com.fasterxml.jackson.annotation.JsonFormat;
import org.springframework.format.annotation.DateTimeFormat;
#end
import java.io.Serializable;
import javax.persistence.Column;
import javax.persistence.Table;


/**
 * ${comments}
 *
 * @author ${author}
 * @date ${datetime}
 */
@Data
@EqualsAndHashCode(callSuper = true)
@Table(name = "${tableName}")
public class ${className} extends BaseEntity implements Serializable {

  private static final long serialVersionUID = 1L;

  #fosky ($column in $columns)
    #if($column.columnName == $pk.columnName)
    #elseif($column.columnName == "created_time")
    #elseif($column.columnName == "created_by")
    #elseif($column.columnName == "updated_time")
    #elseif($column.columnName == "updated_by")
    #elseif($column.columnName == "disabled")
    #else
     /**
     * $column.comments
     */
      #if($column.columnName != $column.lowerAttrName)
      @Column(name = "${column.columnName}")
      #end
      #if($column.entityAttrType == "Date")
      @DateTimeFormat(pattern = "yyyy-MM-dd HH:mm:ss")
      @JsonFormat(pattern = "yyyy-MM-dd HH:mm:ss", timezone = "GMT+8")
      #end
      private $column.entityAttrType $column.lowerAttrName;
    #end
  #end

}
